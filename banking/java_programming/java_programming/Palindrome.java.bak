class Palindrome 
{
	public static void main(String[] args) 
	{
		int num=12321;
		int temp=num;
		int lastdigit=0;
		int reverse=0;
		while (num!=0)
		{
			lastdigit=num%10;
			reverse=reverse*10+lastdigit;
				num=num/10;
		}
		if (reverse==num)
		{
			System.out.println(temp+ " :is palidrome");
		}
		else
		{
			System.out.println(temp+": not palindrome");
		}
	}
}
